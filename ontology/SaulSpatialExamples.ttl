# baseURI: http://ontology.ihmc.us/Spatial/SaulSpatialExamples.owl
# imports: http://ontology.ihmc.us/Spatial/SaulSpatial.owl

@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix saulSpatial:  <http://ontology.ihmc.us/Spatial/SaulSpatial.owl#> .
@prefix saulSpatialExamples:  <http://ontology.ihmc.us/Spatial/SaulSpatialExamples.owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://ontology.ihmc.us/Spatial/SaulSpatialExamples.owl>
      rdf:type owl:Ontology ;
      rdfs:comment """This ontology contains examples of Saul Spatial Configuration definitions using Saul spatial ontology concepts.
     
     Created by Andrzej Uszok (auszok@ihmc.us)."""^^xsd:string ;
      owl:imports <http://ontology.ihmc.us/Spatial/SaulSpatial.owl> .

saulSpatialExamples:e1
      rdf:type saulSpatial:block , saulSpatial:tr ;
      rdfs:label "large red block"^^xsd:string ;
      saulSpatial:col saulSpatial:red ;
      saulSpatial:hasSpan saulSpatialExamples:e1_span ;
      saulSpatial:size saulSpatial:large .

saulSpatialExamples:e1_span
      rdf:type saulSpatial:span ;
      saulSpatial:hasSubText
              "the large red block"^^xsd:string .

saulSpatialExamples:e2
      rdf:type saulSpatial:column ;
      rdfs:label "top bue column"^^xsd:string ;
      saulSpatial:area saulSpatial:top ;
      saulSpatial:col saulSpatial:red ;
      saulSpatial:hasSpan saulSpatialExamples:e2_span .

saulSpatialExamples:e2InRedBlockConfigurationContext
      rdf:type saulSpatial:lmInContext ;
      saulSpatial:hasFoR saulSpatial:relative ;
      saulSpatial:hasLm saulSpatialExamples:e2 ;
      saulSpatial:hasSp saulSpatialExamples:e2InRedBlockConfigurationContext_sp .

saulSpatialExamples:e2InRedBlockConfigurationContext_sp
      rdf:type saulSpatial:sp ;
      saulSpatial:hasSpan saulSpatialExamples:e2InRedBlockConfigurationContext_span .

saulSpatialExamples:e2InRedBlockConfigurationContext_span
      rdf:type saulSpatial:span ;
      saulSpatial:hasSentenceNo
              1 ;
      saulSpatial:hasStartTextIndex
              37 ;
      saulSpatial:hasSubText
              "from"^^xsd:string ;
      saulSpatial:hastEndTextIndex
              40 .

saulSpatialExamples:e2_span
      rdf:type saulSpatial:span ;
      saulSpatial:hasSubText
              "the top of the blue column"^^xsd:string .

saulSpatialExamples:e3
      rdf:type saulSpatial:column ;
      rdfs:label "top yellow column"^^xsd:string ;
      saulSpatial:area saulSpatial:top ;
      saulSpatial:col saulSpatial:yellow ;
      saulSpatial:hasSpan saulSpatialExamples:e3_span .

saulSpatialExamples:e3InRedBlockConfigurationContext
      rdf:type saulSpatial:lmInContext ;
      saulSpatial:hasFoR saulSpatial:relative ;
      saulSpatial:hasLm saulSpatialExamples:e3 ;
      saulSpatial:hasSp saulSpatialExamples:e3InRedBlockConfigurationContext_sp .

saulSpatialExamples:e3InRedBlockConfigurationContext_sp
      rdf:type saulSpatial:sp ;
      saulSpatial:hasSpan saulSpatialExamples:e3InRedBlockConfigurationContext_span .

saulSpatialExamples:e3InRedBlockConfigurationContext_span
      rdf:type saulSpatial:span ;
      saulSpatial:hasSentenceNo
              1 ;
      saulSpatial:hasStartTextIndex
              69 ;
      saulSpatial:hasSubText
              "to"^^xsd:string ;
      saulSpatial:hastEndTextIndex
              71 .

saulSpatialExamples:e3_span
      rdf:type saulSpatial:span ;
      saulSpatial:hasSubText
              "the top of the yellow column"^^xsd:string .

saulSpatialExamples:e4
      rdf:type saulSpatial:tr ;
      rdfs:label "orange cube"^^xsd:string ;
      saulSpatial:col saulSpatial:orange ;
      saulSpatial:hasSpan saulSpatialExamples:e4_span .

saulSpatialExamples:e4InOrangCubeConfigurationContext
      rdf:type saulSpatial:lmInContext ;
      saulSpatial:hasFoR saulSpatial:relative ;
      saulSpatial:hasLm saulSpatialExamples:e4 ;
      saulSpatial:hasSp saulSpatialExamples:e4InOrangeCubeConfigurationContext_sp .

saulSpatialExamples:e4InOrangeCubeConfigurationContext_sp
      rdf:type saulSpatial:sp ;
      saulSpatial:hasSpan saulSpatialExamples:e4InOrangeCubeConfigurationContext_span .

saulSpatialExamples:e4InOrangeCubeConfigurationContext_span
      rdf:type saulSpatial:span ;
      saulSpatial:hasSentenceNo
              1 ;
      saulSpatial:hasStartTextIndex
              111 ;
      saulSpatial:hasSubText
              "5 spaces from"^^xsd:string ;
      saulSpatial:hastEndTextIndex
              123 .

saulSpatialExamples:e4_span
      rdf:type saulSpatial:span ;
      saulSpatial:hasSubText
              "the orange cube"^^xsd:string .

saulSpatialExamples:orangeCubeConfiguration
      rdf:type saulSpatial:spatialConfiguration ;
      rdfs:comment "which is 5 spaces from the orange cube."^^xsd:string ;
      saulSpatial:hasLmInContext
              saulSpatialExamples:e4InOrangCubeConfigurationContext ;
      saulSpatial:hasQT saulSpatial:DC , saulSpatial:distal , saulSpatial:quantitative , saulSpatial:topological ;
      saulSpatial:hasTr saulSpatialExamples:e4 ;
      saulSpatial:hasV saulSpatial:firstPersonAnon .

saulSpatialExamples:redBlockConfiguration
      rdf:type saulSpatial:dynamicSpatialConfiguration ;
      rdfs:comment "Move the large red block diagonally from the top of the blue column to the top of the yellow column" ;
      saulSpatial:hasLmInContext
              saulSpatialExamples:e2InRedBlockConfigurationContext , saulSpatialExamples:e3InRedBlockConfigurationContext ;
      saulSpatial:hasM saulSpatialExamples:redBlockMovment ;
      saulSpatial:hasPath saulSpatialExamples:redBlockPath ;
      saulSpatial:hasQT saulSpatial:directional , saulSpatial:relativeQT ;
      saulSpatial:hasTr saulSpatialExamples:e1 ;
      saulSpatial:hasV saulSpatial:firstPersonAnon .

saulSpatialExamples:redBlockMovment
      rdf:type saulSpatial:m ;
      saulSpatial:manner saulSpatial:diagonally ;
      saulSpatial:verb saulSpatial:move .

saulSpatialExamples:redBlockPath
      rdf:type saulSpatial:path ;
      saulSpatial:hasPathElement
              saulSpatialExamples:redBlockPathBegin , saulSpatialExamples:redBlockPathEnd .

saulSpatialExamples:redBlockPathBegin
      rdf:type saulSpatial:pathElement ;
      saulSpatial:hasPathElementPosition
              saulSpatial:begin ;
      saulSpatial:referLmInContext
              saulSpatialExamples:e2InRedBlockConfigurationContext .

saulSpatialExamples:redBlockPathEnd
      rdf:type saulSpatial:pathElement ;
      saulSpatial:hasPathElementPosition
              saulSpatial:end ;
      saulSpatial:referLmInContext
              saulSpatialExamples:e3InRedBlockConfigurationContext .
