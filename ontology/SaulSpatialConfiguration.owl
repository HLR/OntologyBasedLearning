<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:saulSpatialConfiguration="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl">
  <owl:Ontology rdf:about="">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This ontology contains Saul spatial concepts. Created by Andrzej Uszok (auszok@ihmc.us).</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="G-typeQT">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="QT"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Formal meaning of QT.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="FoR">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A type of the coordinate system to identify location of an object, can be intrinsic, relative or absolute.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >frame-of-reference-type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="spatialEntity">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The entity that participates in spatial configurations with varied roles of tr, lm or a part of a path.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="sp">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The lexical form of the relation between the tr and lm.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >spatial indicator</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="speedOfMovment">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The value of the movment speed.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="span">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The text fragment describing given spatial or motion indicator</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >span</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="areaName">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The indication of the spatial entity area.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="v">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >When FoR is relative, this indicates the viewer as first, second or third person.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >viewer</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="shapeTypes">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The shape of the spatial entity.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="dynamicSpatialConfiguration">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The spatial configuration of the situation discribied in the NL setance or its part including movment of trajector.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="spatialConfiguration"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="pathElement">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The element of the path.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="spatialRole"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="tr">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The entity whose location or trans-location is described in a spatial configuration.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >trajector</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#spatialRole"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="sizeType">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The size of the spatial entity</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="movmentManner">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The manner of movement.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="m">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Spatial movement usually described by a motion verb.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >motion indicator</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="color">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The color of the spatial entity</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="F-typeQT">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#QT"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >General type of the QT.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#QT">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The qualitative - formal type of the relation between the tr and lm.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >qualitative type</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="path">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The tr location can be described via a path of motion instead of a basic lm.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#spatialRole">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Spatial role taken by the spatial enity in the scope of spatial configuraiton.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="pathElementPosition">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Tag depicting position of the path element in the whole path.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="lm">
    <rdfs:subClassOf rdf:resource="#spatialRole"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >landmark</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The reference object that describes the location of the tr or is a part of its path of motion.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#spatialConfiguration">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The spatial configuration of the situation discribied in the NL setance or its part.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasSpan">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#spatialEntity"/>
          <owl:Class rdf:about="#sp"/>
          <owl:Class rdf:about="#m"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#span"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPathElement">
    <rdfs:domain rdf:resource="#path"/>
    <rdfs:range rdf:resource="#pathElement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="region">
    <rdfs:domain rdf:resource="#sp"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPathElementPosition">
    <rdfs:range rdf:resource="#pathElementPosition"/>
    <rdfs:domain rdf:resource="#pathElement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasPath">
    <rdfs:domain rdf:resource="#dynamicSpatialConfiguration"/>
    <rdfs:range rdf:resource="#path"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="direction">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#m"/>
          <owl:Class rdf:about="#sp"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="speed">
    <rdfs:range rdf:resource="#speedOfMovment"/>
    <rdfs:domain rdf:resource="#m"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="distance">
    <rdfs:domain rdf:resource="#sp"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasM">
    <rdfs:domain rdf:resource="#dynamicSpatialConfiguration"/>
    <rdfs:range rdf:resource="#m"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="manner">
    <rdfs:range rdf:resource="#movmentManner"/>
    <rdfs:domain rdf:resource="#m"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasQT">
    <rdfs:domain rdf:resource="#spatialConfiguration"/>
    <rdfs:range rdf:resource="#QT"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="shape">
    <rdfs:range rdf:resource="#shapeTypes"/>
    <rdfs:domain rdf:resource="#spatialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSp">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#spatialConfiguration"/>
          <owl:Class rdf:about="#pathElement"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#sp"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasV">
    <rdfs:domain rdf:resource="#spatialConfiguration"/>
    <rdfs:range rdf:resource="#v"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="hastEndTextIndex">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="#span"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasSpeedValue">
    <rdfs:domain rdf:resource="#speedOfMovment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasStartTextIndex">
    <rdfs:domain rdf:resource="#span"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasSubText">
    <rdfs:domain rdf:resource="#span"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="hasSentenceNo">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="#span"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="hasTr">
    <rdfs:domain rdf:resource="#spatialConfiguration"/>
    <rdfs:range rdf:resource="#tr"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="col">
    <rdfs:range rdf:resource="#color"/>
    <rdfs:domain rdf:resource="#spatialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="size">
    <rdfs:range rdf:resource="#sizeType"/>
    <rdfs:domain rdf:resource="#spatialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasSpatialEntity">
    <rdfs:domain rdf:resource="#spatialRole"/>
    <rdfs:range rdf:resource="#spatialEntity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasFoR">
    <rdfs:range rdf:resource="#FoR"/>
    <rdfs:domain rdf:resource="#lm"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="area">
    <rdfs:domain rdf:resource="#spatialEntity"/>
    <rdfs:range rdf:resource="#areaName"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasLm">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#spatialConfiguration"/>
          <owl:Class rdf:about="#pathElement"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#lm"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <saulSpatialConfiguration:G-typeQT rdf:ID="topological"/>
  <saulSpatialConfiguration:movmentManner rdf:ID="vertically"/>
  <saulSpatialConfiguration:areaName rdf:ID="top"/>
  <saulSpatialConfiguration:FoR rdf:ID="absolute"/>
  <saulSpatialConfiguration:color rdf:ID="black"/>
  <saulSpatialConfiguration:color rdf:ID="green"/>
  <saulSpatialConfiguration:pathElementPosition rdf:ID="middle"/>
  <saulSpatialConfiguration:v rdf:ID="thirdPerson"/>
  <saulSpatialConfiguration:G-typeQT rdf:ID="directional"/>
  <saulSpatialConfiguration:pathElementPosition rdf:ID="whole"/>
  <saulSpatialConfiguration:F-typeQT rdf:ID="DC"/>
  <saulSpatialConfiguration:shapeTypes rdf:ID="column"/>
  <saulSpatialConfiguration:sizeType rdf:ID="large"/>
  <saulSpatialConfiguration:sizeType rdf:ID="small"/>
  <saulSpatialConfiguration:v rdf:ID="firstPerson"/>
  <saulSpatialConfiguration:color rdf:ID="white"/>
  <saulSpatialConfiguration:color rdf:ID="orange"/>
  <saulSpatialConfiguration:G-typeQT rdf:ID="distal"/>
  <saulSpatialConfiguration:v rdf:ID="secondPerson"/>
  <saulSpatialConfiguration:areaName rdf:ID="bottom"/>
  <saulSpatialConfiguration:FoR rdf:ID="intrinsic"/>
  <saulSpatialConfiguration:shapeTypes rdf:ID="cube"/>
  <saulSpatialConfiguration:pathElementPosition rdf:ID="begin"/>
  <saulSpatialConfiguration:F-typeQT rdf:ID="quantitative"/>
  <saulSpatialConfiguration:movmentManner rdf:ID="diagonally"/>
  <saulSpatialConfiguration:shapeTypes rdf:ID="block"/>
  <saulSpatialConfiguration:pathElementPosition rdf:ID="end"/>
  <saulSpatialConfiguration:F-typeQT rdf:ID="relative"/>
  <saulSpatialConfiguration:color rdf:ID="red"/>
  <saulSpatialConfiguration:color rdf:ID="yellow"/>
</rdf:RDF>

