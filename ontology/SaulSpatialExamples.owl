<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:saulSpatialConfiguration="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:saulSpatialExamples="http://ontology.ihmc.us/Spatial/SaulSpatialExamples.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://ontology.ihmc.us/Spatial/SaulSpatialExamples.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This ontology contains examples of Saul Spatial Configuration definitions using Saul spatial ontology concepts.
     
     Created by Andrzej Uszok (auszok@ihmc.us).</rdfs:comment>
  </owl:Ontology>
  <saulSpatialConfiguration:dynamicSpatialConfiguration rdf:ID="Configuration1">
    <saulSpatialConfiguration:hasLm>
      <saulSpatialConfiguration:lm rdf:ID="l2">
        <saulSpatialConfiguration:hasSpatialEntity>
          <saulSpatialConfiguration:spatialEntity rdf:ID="e3">
            <saulSpatialConfiguration:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#yellow"/>
            <saulSpatialConfiguration:shape rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#column"/>
            <saulSpatialConfiguration:area rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#top"/>
            <saulSpatialConfiguration:hasSpan>
              <saulSpatialConfiguration:span rdf:ID="e3_span">
                <saulSpatialConfiguration:hasSubText rdf:datatype=
                "http://www.w3.org/2001/XMLSchema#string">the top of the yellow column</saulSpatialConfiguration:hasSubText>
              </saulSpatialConfiguration:span>
            </saulSpatialConfiguration:hasSpan>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >top yellow column</rdfs:label>
          </saulSpatialConfiguration:spatialEntity>
        </saulSpatialConfiguration:hasSpatialEntity>
        <saulSpatialConfiguration:hasFoR rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#relative"/>
      </saulSpatialConfiguration:lm>
    </saulSpatialConfiguration:hasLm>
    <rdfs:comment>Move the large red block diagonally from the top of the blue column to the top of the yellow column</rdfs:comment>
    <saulSpatialConfiguration:hasM>
      <saulSpatialConfiguration:m rdf:ID="m1">
        <saulSpatialConfiguration:verb rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#move"/>
        <saulSpatialConfiguration:manner rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#diagonally"/>
      </saulSpatialConfiguration:m>
    </saulSpatialConfiguration:hasM>
    <saulSpatialConfiguration:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#relativeQT"/>
    <saulSpatialConfiguration:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#directional"/>
    <saulSpatialConfiguration:hasSp>
      <saulSpatialConfiguration:sp rdf:ID="s1">
        <saulSpatialConfiguration:hasSpan>
          <saulSpatialConfiguration:span rdf:ID="s1_span">
            <saulSpatialConfiguration:hasSentenceNo rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">1</saulSpatialConfiguration:hasSentenceNo>
            <saulSpatialConfiguration:hastEndTextIndex rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">40</saulSpatialConfiguration:hastEndTextIndex>
            <saulSpatialConfiguration:hasStartTextIndex rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">37</saulSpatialConfiguration:hasStartTextIndex>
            <saulSpatialConfiguration:hasSubText rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#string">from</saulSpatialConfiguration:hasSubText>
          </saulSpatialConfiguration:span>
        </saulSpatialConfiguration:hasSpan>
      </saulSpatialConfiguration:sp>
    </saulSpatialConfiguration:hasSp>
    <saulSpatialConfiguration:hasV rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#firstPersonAnon"/>
    <saulSpatialConfiguration:hasSp>
      <saulSpatialConfiguration:sp rdf:ID="s2">
        <saulSpatialConfiguration:hasSpan>
          <saulSpatialConfiguration:span rdf:ID="s2_span">
            <saulSpatialConfiguration:hasStartTextIndex rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">69</saulSpatialConfiguration:hasStartTextIndex>
            <saulSpatialConfiguration:hastEndTextIndex rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">71</saulSpatialConfiguration:hastEndTextIndex>
            <saulSpatialConfiguration:hasSentenceNo rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">1</saulSpatialConfiguration:hasSentenceNo>
            <saulSpatialConfiguration:hasSubText rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#string">to</saulSpatialConfiguration:hasSubText>
          </saulSpatialConfiguration:span>
        </saulSpatialConfiguration:hasSpan>
      </saulSpatialConfiguration:sp>
    </saulSpatialConfiguration:hasSp>
    <saulSpatialConfiguration:hasTr>
      <saulSpatialConfiguration:tr rdf:ID="t1">
        <saulSpatialConfiguration:hasSpatialEntity>
          <saulSpatialConfiguration:spatialEntity rdf:ID="e1">
            <saulSpatialConfiguration:shape rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#block"/>
            <saulSpatialConfiguration:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#red"/>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >large red block</rdfs:label>
            <saulSpatialConfiguration:hasSpan>
              <saulSpatialConfiguration:span rdf:ID="e1_span">
                <saulSpatialConfiguration:hasSubText rdf:datatype=
                "http://www.w3.org/2001/XMLSchema#string">the large red block</saulSpatialConfiguration:hasSubText>
              </saulSpatialConfiguration:span>
            </saulSpatialConfiguration:hasSpan>
            <saulSpatialConfiguration:size rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#large"/>
          </saulSpatialConfiguration:spatialEntity>
        </saulSpatialConfiguration:hasSpatialEntity>
      </saulSpatialConfiguration:tr>
    </saulSpatialConfiguration:hasTr>
    <saulSpatialConfiguration:hasLm>
      <saulSpatialConfiguration:lm rdf:ID="l1">
        <saulSpatialConfiguration:hasSpatialEntity>
          <saulSpatialConfiguration:spatialEntity rdf:ID="e2">
            <saulSpatialConfiguration:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#red"/>
            <saulSpatialConfiguration:shape rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#column"/>
            <saulSpatialConfiguration:hasSpan>
              <saulSpatialConfiguration:span rdf:ID="e2_span">
                <saulSpatialConfiguration:hasSubText rdf:datatype=
                "http://www.w3.org/2001/XMLSchema#string">the top of the blue column</saulSpatialConfiguration:hasSubText>
              </saulSpatialConfiguration:span>
            </saulSpatialConfiguration:hasSpan>
            <saulSpatialConfiguration:area rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#top"/>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >top bue column</rdfs:label>
          </saulSpatialConfiguration:spatialEntity>
        </saulSpatialConfiguration:hasSpatialEntity>
        <saulSpatialConfiguration:hasFoR rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#relative"/>
      </saulSpatialConfiguration:lm>
    </saulSpatialConfiguration:hasLm>
    <saulSpatialConfiguration:hasPath>
      <saulSpatialConfiguration:path rdf:ID="m1Path">
        <saulSpatialConfiguration:hasPathElement>
          <saulSpatialConfiguration:pathElement rdf:ID="m1PathElement2">
            <saulSpatialConfiguration:hasSp rdf:resource="#s2"/>
            <saulSpatialConfiguration:hasLm rdf:resource="#l2"/>
            <saulSpatialConfiguration:hasPathElementPosition rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#end"/>
          </saulSpatialConfiguration:pathElement>
        </saulSpatialConfiguration:hasPathElement>
        <saulSpatialConfiguration:hasPathElement>
          <saulSpatialConfiguration:pathElement rdf:ID="m1PathElement1">
            <saulSpatialConfiguration:hasPathElementPosition rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#begin"/>
            <saulSpatialConfiguration:hasSp rdf:resource="#s1"/>
            <saulSpatialConfiguration:hasLm rdf:resource="#l1"/>
          </saulSpatialConfiguration:pathElement>
        </saulSpatialConfiguration:hasPathElement>
      </saulSpatialConfiguration:path>
    </saulSpatialConfiguration:hasPath>
  </saulSpatialConfiguration:dynamicSpatialConfiguration>
  <saulSpatialConfiguration:spatialConfiguration rdf:ID="Configuration2">
    <saulSpatialConfiguration:hasV rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#firstPersonAnon"/>
    <saulSpatialConfiguration:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#quantitative"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >which is 5 spaces from the orange cube.</rdfs:comment>
    <saulSpatialConfiguration:hasTr>
      <saulSpatialConfiguration:tr rdf:ID="t2">
        <saulSpatialConfiguration:hasSpatialEntity rdf:resource="#e3"/>
      </saulSpatialConfiguration:tr>
    </saulSpatialConfiguration:hasTr>
    <saulSpatialConfiguration:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#topological"/>
    <saulSpatialConfiguration:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#distal"/>
    <saulSpatialConfiguration:hasLm>
      <saulSpatialConfiguration:lm rdf:ID="l3">
        <saulSpatialConfiguration:hasSpatialEntity>
          <saulSpatialConfiguration:spatialEntity rdf:ID="e4">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >orange cube</rdfs:label>
            <saulSpatialConfiguration:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#orange"/>
            <saulSpatialConfiguration:hasSpan>
              <saulSpatialConfiguration:span rdf:ID="e4_span">
                <saulSpatialConfiguration:hasSubText rdf:datatype=
                "http://www.w3.org/2001/XMLSchema#string">the orange cube</saulSpatialConfiguration:hasSubText>
              </saulSpatialConfiguration:span>
            </saulSpatialConfiguration:hasSpan>
            <saulSpatialConfiguration:shape rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#cube"/>
          </saulSpatialConfiguration:spatialEntity>
        </saulSpatialConfiguration:hasSpatialEntity>
        <saulSpatialConfiguration:hasFoR rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#relative"/>
      </saulSpatialConfiguration:lm>
    </saulSpatialConfiguration:hasLm>
    <saulSpatialConfiguration:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatialConfiguration.owl#DC"/>
    <saulSpatialConfiguration:hasSp>
      <saulSpatialConfiguration:sp rdf:ID="s3">
        <saulSpatialConfiguration:hasSpan>
          <saulSpatialConfiguration:span rdf:ID="s3_span">
            <saulSpatialConfiguration:hastEndTextIndex rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">123</saulSpatialConfiguration:hastEndTextIndex>
            <saulSpatialConfiguration:hasSentenceNo rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">1</saulSpatialConfiguration:hasSentenceNo>
            <saulSpatialConfiguration:hasStartTextIndex rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#integer">111</saulSpatialConfiguration:hasStartTextIndex>
            <saulSpatialConfiguration:hasSubText rdf:datatype=
            "http://www.w3.org/2001/XMLSchema#string">5 spaces from</saulSpatialConfiguration:hasSubText>
          </saulSpatialConfiguration:span>
        </saulSpatialConfiguration:hasSpan>
      </saulSpatialConfiguration:sp>
    </saulSpatialConfiguration:hasSp>
  </saulSpatialConfiguration:spatialConfiguration>
</rdf:RDF>

