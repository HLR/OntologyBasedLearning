<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:saulSpatial="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:saulSpatialExamples="http://ontology.ihmc.us/Spatial/SaulSpatialExamples.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://ontology.ihmc.us/Spatial/SaulSpatialExamples.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >This ontology contains examples of Saul Spatial Configuration definitions using Saul spatial ontology concepts.
     
     Created by Andrzej Uszok (auszok@ihmc.us).</rdfs:comment>
  </owl:Ontology>
  <saulSpatial:spatialConfiguration rdf:ID="orangeCubeConfiguration">
    <saulSpatial:hasLmInContext>
      <saulSpatial:lmInContext rdf:ID="e4InOrangCubeConfigurationContext">
        <saulSpatial:hasSp>
          <saulSpatial:sp rdf:ID="e4InOrangeCubeConfigurationContext_sp">
            <saulSpatial:hasSpan>
              <saulSpatial:span rdf:ID="e4InOrangeCubeConfigurationContext_span">
                <saulSpatial:hastEndTextIndex rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >123</saulSpatial:hastEndTextIndex>
                <saulSpatial:hasSentenceNo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >1</saulSpatial:hasSentenceNo>
                <saulSpatial:hasStartTextIndex rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >111</saulSpatial:hasStartTextIndex>
                <saulSpatial:hasSubText rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >5 spaces from</saulSpatial:hasSubText>
              </saulSpatial:span>
            </saulSpatial:hasSpan>
          </saulSpatial:sp>
        </saulSpatial:hasSp>
        <saulSpatial:hasFoR rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#relative"/>
        <saulSpatial:hasLm>
          <saulSpatial:tr rdf:ID="e4">
            <saulSpatial:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#orange"/>
            <saulSpatial:hasSpan>
              <saulSpatial:span rdf:ID="e4_span">
                <saulSpatial:hasSubText rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >the orange cube</saulSpatial:hasSubText>
              </saulSpatial:span>
            </saulSpatial:hasSpan>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >orange cube</rdfs:label>
          </saulSpatial:tr>
        </saulSpatial:hasLm>
      </saulSpatial:lmInContext>
    </saulSpatial:hasLmInContext>
    <saulSpatial:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#topological"/>
    <saulSpatial:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#distal"/>
    <saulSpatial:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#quantitative"/>
    <saulSpatial:hasV rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#firstPersonAnon"/>
    <saulSpatial:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#DC"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >which is 5 spaces from the orange cube.</rdfs:comment>
    <saulSpatial:hasTr rdf:resource="#e4"/>
  </saulSpatial:spatialConfiguration>
  <saulSpatial:dynamicSpatialConfiguration rdf:ID="redBlockConfiguration">
    <saulSpatial:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#directional"/>
    <saulSpatial:hasM>
      <saulSpatial:m rdf:ID="redBlockMovment">
        <saulSpatial:verb rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#move"/>
        <saulSpatial:manner rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#diagonally"/>
      </saulSpatial:m>
    </saulSpatial:hasM>
    <saulSpatial:hasLmInContext>
      <saulSpatial:lmInContext rdf:ID="e2InRedBlockConfigurationContext">
        <saulSpatial:hasSp>
          <saulSpatial:sp rdf:ID="e2InRedBlockConfigurationContext_sp">
            <saulSpatial:hasSpan>
              <saulSpatial:span rdf:ID="e2InRedBlockConfigurationContext_span">
                <saulSpatial:hasSubText rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >from</saulSpatial:hasSubText>
                <saulSpatial:hasStartTextIndex rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >37</saulSpatial:hasStartTextIndex>
                <saulSpatial:hasSentenceNo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >1</saulSpatial:hasSentenceNo>
                <saulSpatial:hastEndTextIndex rdf:datatype="http://www.w3.org/2001/XMLSchema#integer"
                >40</saulSpatial:hastEndTextIndex>
              </saulSpatial:span>
            </saulSpatial:hasSpan>
          </saulSpatial:sp>
        </saulSpatial:hasSp>
        <saulSpatial:hasLm>
          <saulSpatial:column rdf:ID="e2">
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >top bue column</rdfs:label>
            <saulSpatial:hasSpan>
              <saulSpatial:span rdf:ID="e2_span">
                <saulSpatial:hasSubText rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >the top of the blue column</saulSpatial:hasSubText>
              </saulSpatial:span>
            </saulSpatial:hasSpan>
            <saulSpatial:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#red"/>
            <saulSpatial:area rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#top"/>
          </saulSpatial:column>
        </saulSpatial:hasLm>
        <saulSpatial:hasFoR rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#relative"/>
      </saulSpatial:lmInContext>
    </saulSpatial:hasLmInContext>
    <saulSpatial:hasV rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#firstPersonAnon"/>
    <saulSpatial:hasQT rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#relativeQT"/>
    <saulSpatial:hasPath>
      <saulSpatial:path rdf:ID="redBlockPath">
        <saulSpatial:hasPathElement>
          <saulSpatial:pathElement rdf:ID="redBlockPathBegin">
            <saulSpatial:hasPathElementPosition rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#begin"/>
            <saulSpatial:referLmInContext rdf:resource="#e2InRedBlockConfigurationContext"/>
          </saulSpatial:pathElement>
        </saulSpatial:hasPathElement>
        <saulSpatial:hasPathElement>
          <saulSpatial:pathElement rdf:ID="redBlockPathEnd">
            <saulSpatial:hasPathElementPosition rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#end"/>
            <saulSpatial:referLmInContext>
              <saulSpatial:lmInContext rdf:ID="e3InRedBlockConfigurationContext">
                <saulSpatial:hasSp>
                  <saulSpatial:sp rdf:ID="e3InRedBlockConfigurationContext_sp">
                    <saulSpatial:hasSpan>
                      <saulSpatial:span rdf:ID="e3InRedBlockConfigurationContext_span">
                        <saulSpatial:hasStartTextIndex rdf:datatype=
                        "http://www.w3.org/2001/XMLSchema#integer"
                        >69</saulSpatial:hasStartTextIndex>
                        <saulSpatial:hasSubText rdf:datatype=
                        "http://www.w3.org/2001/XMLSchema#string"
                        >to</saulSpatial:hasSubText>
                        <saulSpatial:hasSentenceNo rdf:datatype=
                        "http://www.w3.org/2001/XMLSchema#integer"
                        >1</saulSpatial:hasSentenceNo>
                        <saulSpatial:hastEndTextIndex rdf:datatype=
                        "http://www.w3.org/2001/XMLSchema#integer"
                        >71</saulSpatial:hastEndTextIndex>
                      </saulSpatial:span>
                    </saulSpatial:hasSpan>
                  </saulSpatial:sp>
                </saulSpatial:hasSp>
                <saulSpatial:hasFoR rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#relative"/>
                <saulSpatial:hasLm>
                  <saulSpatial:column rdf:ID="e3">
                    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >top yellow column</rdfs:label>
                    <saulSpatial:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#yellow"/>
                    <saulSpatial:hasSpan>
                      <saulSpatial:span rdf:ID="e3_span">
                        <saulSpatial:hasSubText rdf:datatype=
                        "http://www.w3.org/2001/XMLSchema#string"
                        >the top of the yellow column</saulSpatial:hasSubText>
                      </saulSpatial:span>
                    </saulSpatial:hasSpan>
                    <saulSpatial:area rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#top"/>
                  </saulSpatial:column>
                </saulSpatial:hasLm>
              </saulSpatial:lmInContext>
            </saulSpatial:referLmInContext>
          </saulSpatial:pathElement>
        </saulSpatial:hasPathElement>
      </saulSpatial:path>
    </saulSpatial:hasPath>
    <rdfs:comment>Move the large red block diagonally from the top of the blue column to the top of the yellow column</rdfs:comment>
    <saulSpatial:hasLmInContext rdf:resource="#e3InRedBlockConfigurationContext"/>
    <saulSpatial:hasTr>
      <saulSpatial:tr rdf:ID="e1">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >large red block</rdfs:label>
        <saulSpatial:col rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#red"/>
        <saulSpatial:hasSpan>
          <saulSpatial:span rdf:ID="e1_span">
            <saulSpatial:hasSubText rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >the large red block</saulSpatial:hasSubText>
          </saulSpatial:span>
        </saulSpatial:hasSpan>
        <saulSpatial:size rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#large"/>
        <rdf:type rdf:resource="http://ontology.ihmc.us/Spatial/SaulSpatial.owl#block"/>
      </saulSpatial:tr>
    </saulSpatial:hasTr>
  </saulSpatial:dynamicSpatialConfiguration>
</rdf:RDF>

